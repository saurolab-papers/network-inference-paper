        <listOfModifiers>
          <modifierSpeciesReference metaid="metaid_19" species="PRR"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> k1 </ci>
              <ci> PRR </ci>
            </apply>
          </math>
          <listOfParameters>
            <parameter metaid="metaid_15" id="k1" name="k1" value="0.1"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
